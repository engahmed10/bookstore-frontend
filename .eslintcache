react-redux-bookstore/bookstore-frontend/src/components/booksComponent/Home.js",["132","133"],"import React,{Component}  from 'react'\nimport {connect} from 'react-redux'\nimport fetchBooks  from '../../actions/fetchBooks'\nimport FrontBook from './frontBook'\nimport BackBook from './backBook'\nimport Flippy, { FrontSide, BackSide } from 'react-flippy';\nimport SearchShow from '../searchShow'\nimport SearchBack from '../searchBack'\n\n class  Home extends Component{\n   \n    \n\n  componentDidMount(){\n    console.log('a')\n     this.props.fetchBooks()\n     console.log('b')\n  }\n\n  componentWillUnmount() {\n        this.props.resetState({type:\"RESET_STATE\"})\n   }\n\n  renderBooks=()=>{\n  \n    if( this.props.booksReducer.searchbooks && this.props.booksReducer.searchbooks.length ){\n        return this.props.booksReducer.searchbooks.map(book=> \n          ( \n      \n          <Flippy \n             flipOnHover={false}\n             flipOnClick={true} \n             flipDirection=\"horizontal\" \n             ref={(r) => this.flippy = r} \n\n          style={{  display: 'grid',width: '200px',float: 'left',\n                    border: '1px solid whitesmoke','box-shadow': '5px 5px 15px',\n                    border: '1px solid whitesmoke', 'text-align': 'center',height: '300px', \n                    margin: '2em', padding: '2em'}} \n           >\n            <FrontSide\n              style={{\n              height: '300px',width: '200px'\n              }}\n            >\n                <SearchShow props={book.volumeInfo} />\n            </FrontSide>\n            <BackSide \n              style={{\n              height: '300px',width: '200px'\n              }}\n            >\n            <SearchBack props={book.volumeInfo} />\n            </BackSide>\n       </Flippy>\n\n        ))\n    }\n       return this.props.booksReducer.books.map(book=> \n      ( \n      <Flippy \n           flipOnHover={false}\n            flipOnClick={true} \n           flipDirection=\"horizontal\" \n           ref={(r) => this.flippy = r} \n\n          style={{  display: 'grid',width: '200px',float: 'left',\n          border: '1px solid whitesmoke','box-shadow': '5px 5px 15px rgba(0,0,0,0.9)',\n           border: '1px solid whitesmoke', 'text-align': 'center',height: '300px', \n            margin: '2em', padding: '2em'}} \n       >\n            <FrontSide\n              style={{\n              height: '300px',width: '200px'\n              }}\n            >\n                <FrontBook props={book} />\n            </FrontSide>\n            <BackSide \n              style={{\n              height: '300px',width: '200px'\n              }}\n            >\n              <BackBook props={book}   />\n            </BackSide>\n       </Flippy>\n      ))\n           \n }\n\n   render(){\n          \n      return ( \n            <div className=\"container\">\n               {this.renderBooks()}\n            </div>\n          )\n    }\n     \n}\n\n const mapStateToProps=(state)=>{\n    return {booksReducer:state.books}\n }\n\n const mapDispatchToProps=dispatch=>{\n    return {\n           fetchBooks:()=> dispatch(fetchBooks()),\n           resetState:(type)=>dispatch(type)\n        }\n }\n    \n export default connect(mapStateToProps,mapDispatchToProps)(Home)","/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/Reducers/booksReducer.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/actions/autoLoginUser.js",["134"]," const autoLoginUser =()=>{\n        const token= localStorage.getItem(\"token\")\n\n    return (dispatch)=>{\n\n      \n      if (token != 'undefined'){\n\n              fetch('http://127.0.0.1:3000/api/v1/autologin',\n              {\n                method:'GET',\n                  headers:{\n                       'Content-Type': 'application/json',\n                       'Accept': 'application/json',\n                       'Autorization':`Bearer ${token}`\n                  },\n                  }\n              )\n              .then(response=>response.json())\n              .then(data=>{\n\n                  if(data.error){\n                   console.log('error')\n                  }else{\n                  localStorage.setItem('token',data.token)\n                 dispatch({type:\"LOGIN_USER\",user: data.user}) \n                  }\n  \n              })\n              .catch((error)=>{\n                  console.error('Error',error)\n               }\n              )\n        }\n    }\n}\n\n\nexport default autoLoginUser","/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/actions/loginUserAction.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/components/booksComponent/backBook.js",["135","136","137","138"]," const BackBook=({props})=>{\n     const{id,image,title,authors,price,isbn,year,}=props\n    return(  \n            <div className=\"\" >\n              <div >\n                  <h5>${price}</h5>\n                  <p >ISBN: {isbn}</p>\n                  <p >Publish Year: {year}</p>\n              </div>\n            </div>\n              \n          )\n}\nexport default BackBook\n\n","/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/index.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/Reducers/index.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/Reducers/userReducer.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/components/NavBar.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/components/authentications/Login.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/components/authentications/SignUpUser.js",[],"/Users/ahmedafandy/Flatiron/code/react-redux-bookstore/bookstore-frontend/src/actions/fetchBooks.js",[],"/Users/ahmedafandy/Flatiron/code/react-r